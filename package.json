{
  "name": "trash-tac-toe",
  "version": "0.1.0",
  "description": "A tic-tac-toe game on Gorbagana Chain featuring trash cans vs recycling bins",
  "main": "index.js",
  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "test": "anchor test --skip-local-validator",
    "test-local": "anchor test",
    "build": "anchor build",
    "deploy": "anchor deploy",
    "deploy-mainnet": "anchor deploy --provider.cluster mainnet",
    "sync-types": "anchor run sync-types"
  },
  "keywords": [
    "solana",
    "anchor",
    "tic-tac-toe",
    "blockchain",
    "game",
    "gorbagana"
  ],
  "author": "Gorbagana Trash-Tac-Toe Team",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.3.5"
  }
} 